package Vectors;

import java.sql.ResultSet;
import java.util.ArrayList;

/**
 * @author mawo
 *
 */
public class Zeitreihe implements FeatureVector {
	
	ResultSet rs;
	ArrayList<String> names;
	
	public Zeitreihe(ResultSet resultset){
		rs = resultset;
		names = new ArrayList();
		for (int i = -10; i < 0; i++) names.add("Wirkleistung t-" + i);
		for (int i = 0; i <= 10; i++) names. add("Wirkleistung t+" + i);
	}

	/* (non-Javadoc)
	 * @see Vectors.FeatureVector#getFeatureVector(int)
	 */
	@Override
	public ArrayList getFeatureVector(int t) {
		// TODO Auto-generated method stub
		
		return null;
	}

	/* (non-Javadoc)
	 * @see Vectors.FeatureVector#listFeatures()
	 */
	@Override
	public ArrayList listFeatures() {
		// TODO Auto-generated method stub
		
		return null;
	}

}
